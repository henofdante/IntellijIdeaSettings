<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="MongoDB-JSON-Query" injector-id="java">
      <display-name>CompoundIndex.def (org.springframework.data.mongodb.core.index)</display-name>
      <single-file value="false" />
      <place><![CDATA[psiMethod().withName("def").withParameters().definedInClass("org.springframework.data.mongodb.core.index.CompoundIndex")]]></place>
    </injection>
    <injection language="SpEL" injector-id="java">
      <display-name>Retryable.maxAttemptsExpression (org.springframework.retry.annotation)</display-name>
      <single-file value="false" />
      <place><![CDATA[psiMethod().withName("maxAttemptsExpression").withParameters().definedInClass("org.springframework.retry.annotation.Retryable")]]></place>
    </injection>
    <injection language="MySQL" injector-id="java">
      <display-name>Update.value (org.apache.ibatis.annotations)</display-name>
      <single-file value="false" />
      <place><![CDATA[psiMethod().withName("value").withParameters().definedInClass("org.apache.ibatis.annotations.Update")]]></place>
    </injection>
    <injection language="SQL" injector-id="xml">
      <display-name>MyBatis select|insert|update|delete</display-name>
      <single-file value="true" />
      <place><![CDATA[xmlTag().withLocalName(string().oneOf("select", "insert", "update", "delete")).withNamespace(string().oneOf("http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd", "http://mybatis.org/dtd/mybatis-3-mapper.dtd"))]]></place>
      <apply-to-subtags />
    </injection>
    <injection language="SQL" injector-id="xml">
      <display-name>MyBatis select|insert|update|delete|statement</display-name>
      <single-file value="true" />
      <place><![CDATA[xmlTag().withLocalName(string().oneOf("select", "insert", "update", "delete", "statement")).withNamespace(string().equalTo("http://ibatis.apache.org/dtd/sql-map-2.dtd"))]]></place>
      <apply-to-subtags />
    </injection>
  </component>
</application>